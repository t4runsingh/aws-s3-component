    "xml2js": "0.4.23"
  },
  "devDependencies": {
    "@elastic.io/component-logger": "0.0.1",
    "chai": "4.2.0",
    "dotenv": "8.2.0",
    "eslint": "7.1.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-plugin-import": "2.20.2",
    "mocha": "7.2.0",
    "nock": "12.0.3",
    "querystring": "0.2.0",
    "sinon": "9.0.2"

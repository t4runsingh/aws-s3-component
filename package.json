{
  "name": "amazon-s3-component",
  "version": "1.3.0",
  "description": "elastic.io integration component that can read and write to AWS S3",
  "homepage": "http://elastic.io",
  "author": {
    "name": "elastic.io GmbH",
    "email": "info@elastic.io",
    "url": "http://elastic.io"
  },
  "engines": {
    "npm": ">=8"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "scripts": {
    "pretest": "echo 'nsp check' && eslint lib spec spec-integration verifyCredentials.js --fix",
    "test": "mocha spec --recursive --timeout 50000",
    "integration-test": "mocha spec-integration/** --timeout 50000"
  },
  "keywords": [
    "aws",
    "s3",
    "ipaas",
    "integration",
    "elasticio-component"
  ],
  "dependencies": {
    "@elastic.io/component-commons-library": "0.0.7",
    "@elastic.io/oih-standard-library": "1.0.1",
    "aws-sdk": "2.314.0",
    "bunyan": "1.8.12",
    "co": "4.6.0",
    "csv": "0.4.6",
    "debug": "2.6.9",
    "elasticio-node": "0.0.9",
    "elasticio-sailor-nodejs": "2.5.4",
    "iconv-lite": "0.4.24",
    "lodash": "4.17.11",
    "mime-types": "2.1.21",
    "q": "1.5.1",
    "remove-leading-slash": "1.0.1",
    "remove-trailing-slash": "0.1.0",
    "request": "2.88.0",
    "requestretry": "3.1.0",
    "s3-upload-stream": "1.0.7",
    "stream": "0.0.2",
    "xml-js": "1.6.8",
    "xml-parse-from-string": "1.0.1",
    "xml2js": "0.4.19"
  },
  "devDependencies": {
    "@elastic.io/component-logger": "0.0.1",
    "chai": "4.1.2",
    "dotenv": "5.0.0",
    "eslint": "6.4.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-plugin-import": "2.18.2",
    "mocha": "5.0.5",
    "nock": "9.6.1",
    "querystring": "0.2.0",
    "sinon": "5.1.0"
  },
  "repository": "elasticio/amazon-s3-component",
  "license": "Apache-2.0"
}

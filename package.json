  "dependencies": {
    "@elastic.io/component-commons-library": "1.1.2",
    "@elastic.io/oih-standard-library": "1.0.3",
    "aws-sdk": "2.683.0",
    "bunyan": "1.8.12",
    "co": "4.6.0",
    "csv": "5.3.2",
    "debug": "4.1.1",
    "elasticio-node": "0.0.9",
    "elasticio-sailor-nodejs": "2.6.8",
    "iconv-lite": "0.5.1",
    "lodash": "4.17.15",
    "mime-types": "2.1.27",
    "q": "1.5.1",
    "remove-leading-slash": "1.0.1",
    "remove-trailing-slash": "0.1.0",
    "request": "2.88.2",
    "requestretry": "4.1.1",
    "s3-upload-stream": "1.0.7",
